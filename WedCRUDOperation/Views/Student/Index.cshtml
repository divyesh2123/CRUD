@model List<Student>

    <table>
@foreach(var t in Model)
{
    <tr>
        <td>@t.Id</td> 
         <td>@t.FirstName</td> 
          <td>@t.LastName</td> 
           <td>@t.Address</td> 
         <td>@t.Email</td> 
         <td><a asp-action="DeleteInfo" asp-controller="Student" asp-route-id="@t.Id">Delete</a> </td>
         <td><a asp-action="Edit" asp-controller="Student" asp-route-id="@t.Id">Edit</a> </td>
    </tr>
}
</table>